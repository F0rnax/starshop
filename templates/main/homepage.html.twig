{% extends 'base.html.twig' %}




{% block body %}
<strong>Starshop</strong>

<div>
Please browse through our {{ ships|length * 10 }} Starships!
</div>

<div>

    <h2>My Ship</h2>

    <table>
        <tr>
            <th>Name:</th>
            <td>{{myShip.name}}</td>
        </tr>
        <tr>
            <th>Class:</th>
            <td>{{myShip.class}}</td>
        </tr>
        <tr>
            <th>Captain:</th>
            <td>{{myShip.captain}}</td>
        </tr>
        <tr>
            <th>Status:</th>
            <td>{{myShip.status}}</td>
        </tr>
    </table>
</div>

{% endblock %}